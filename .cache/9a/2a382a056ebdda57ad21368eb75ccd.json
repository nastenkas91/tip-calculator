zing: border-box;\n  text-align: end;\n  width: 100%;\n  height: 36px;\n  padding: 12px;\n  margin-top: 8px;\n  font-size: $inputFont;\n  color: $VeryDarkCyan;\n  background-color: $VeryLightGrayishCyan;\n  border: none;\n  border-radius: 8px;\n  position: relative;\n\n    &:focus {\n      outline: none;\n      border: 1px solid $primary;\n    }\n}\n\n.total {\n  &::before {\n    content: '';\n    background-image: url(\"../images/icon-dollar.svg\");\n    width: 11px;\n    height: 17px;\n    position: absolute;\n    left: 12px;\n    top: 34px;\n    background-repeat: no-repeat;\n  }\n}\n\n.persons {\n  &::before {\n    content: '';\n    background-image: url(\"../images/icon-person.svg\");\n    position: absolute;\n    width: 15px;\n    height: 17px;\n    left: 12px;\n    top: 34px;\n    background-repeat: no-repeat;\n  }\n}\n\n.discount-container {\n  @include flex-row;\n  gap: 8px;\n  flex-wrap: wrap;\n}\n\n.discount-list-item {\n  padding: 0;\n}\n\n.button {\n  border-radius: 8px;\n  border: none;\n\n    &.discount {\n      background: $VeryDarkCyan;\n      color: $VeryLightGrayishCyan;\n      width: 80px;\n      height: 35px;\n    }\n\n    &.custom {\n      color: $GrayishCyan;\n      width: 80px;\n      height: 35px;\n      background: $VeryLightGrayishCyan;\n    }\n\n    &.reset {\n      background-color: $primary;\n      width: 100%;\n      height: 35px;\n      color: $VeryDarkCyan;\n    }\n\n}\n\n.results-container {\n  @include flex-column;\n  justify-content: space-between;\n  background-color: $VeryDarkCyan;\n  box-sizing: border-box;\n  padding: 24px;\n  border-radius: 24px;\n}\n\n.list {\n  @include flex-column;\n  gap: 24px;\n}\n\n.title-container {\n  @include flex-column;\n  justify-content: center;\n}\n\n.result {\n  @include flex-row;\n  justify-content: space-between;\n\n    .title {\n      font-size: 14px;\n      color: $VeryLightGrayishCyan;\n      margin: 0;\n    }\n\n    .span {\n      font-size: 10px;\n      color: $GrayishCyan;\n    }\n\n    .sum {\n      font-size: 36px;\n      color: $primary;\n      margin: 0;\n    }\n}\n\n@media screen and (max-width: 670px) {\n  .main-container {\n    grid-template-columns: unset;\n    grid-template-rows: repeat(2, auto);\n    max-width: 375px;\n    width: 100%;\n  }\n\n  .parameters-container {\n    max-width: 375px;\n    width: 100%;\n  }\n\n  .results-container {\n    max-width: 375px;\n    width: 100%;\n  }\n\n  .discount-container {\n    display: grid;\n    grid-template-columns: repeat(2, 1fr);\n    gap: 16px;\n    flex-wrap: wrap;\n  }\n\n  .button {\n    font-size: 18px;\n    &.discount {\n      width: 100%;\n      height: 40px;\n    }\n\n    &.custom {\n      width: 100%;\n      height: 40px;\n    }\n  }\n}\n  //- Mobile: 375px\n  //- Desktop: 1440px","stylesheets/abstracts/_mixins.scss":"@mixin flex-column {\n  display: flex;\n  flex-direction: column;\n}\n\n@mixin flex-row {\n  display: flex;\n  flex-direction: row;\n}\n\n@mixin flex-center {\n  justify-content: center;\n  align-content: center;\n}\n"},"lineCount":null}},"error":null,"hash":"ecbaebd16904adf846942ffccbbd28dc","cacheData":{"env":{}}}